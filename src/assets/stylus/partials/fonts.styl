@font-face
	font-family 'PTSans'
	src: url('../fonts/PT Sans.woff2') format('woff2'),
				url('../fonts/PT Sans.woff') format('woff')
	font-weight normal
	font-style normal
	font-display swap

@font-face
	font-family 'PTSansBold'
	src: url('../fonts/PT Sans Bold.woff2') format('woff2'),
				url('../fonts/PT Sans Bold.woff') format('woff')
	font-weight normal
	font-style normal
	font-display swap
